<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Rooster.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Rooster"
    Shell.FlyoutBehavior="Disabled"
    Title="Rooster">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Toevoegen"
        ContentTemplate="{DataTemplate local:Toevoegen}"
        Route="Toevoegen"/>
</Shell>
